{
  "name": "pollen-solutions/social",
  "description": "Social Components",
  "homepage": "https://github.com/milkcreation/pollen-social.git",
  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "social"
  ],
  "time": "2021-09-15",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "hybridauth/hybridauth": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Social\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Social\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}

