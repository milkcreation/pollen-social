{
  "name": "pollen-solutions/social",
  "description": "Social Components",
  "homepage": "https://www.presstify.com/pollen-solutions/social",
  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "social"
  ],
  "type": "presstify-plugin",
  "time": "2020-12-26",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "hybridauth/hybridauth": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Social\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Social\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}

